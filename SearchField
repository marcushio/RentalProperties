  private void SearchFieldActionPerformed(java.awt.event.ActionEvent evt) {                                            
        try {
          
          String query = ("select * from Rentals.Tenant where LastName =? ");
          PreparedStatement search = con.prepareStatement (query);
          search.setString(1, SearchField.getText());
          ResultSet rs = search.executeQuery();
          
          if (rs.next()) {
          
        String ID = rs.getString(1);
        String LastName = rs.getString(2);
        String FirstName = rs.getString(3);
        String PhoneNumber = rs.getString(4);
        String RentPaid = rs.getString(5);
        String Email = rs.getString(6);
            
        /*search.setString(1,ID);
        search.setString(2,LastName);
        search.setString(3,FirstName);
        search.setString(4,PhoneNumber);
        search.setString(5,RentPaid);
        search.setString(6,Email);
          //ResultSet row = search.executeQuery();*/
        
        IDTxt.setText(ID);
         LNTxt.setText(LastName);
        FNTxt.setText(FirstName);
        PNTxt.setText(PhoneNumber);
        RPTxt.setText(RentPaid);
        ETxt.setText(Email);
           //Tenants2.setModel(DbUtils.resultSetToTableModel(rs)); 
          }

 
                }
      catch (SQLException E) {
          E.printStackTrace();
      }
      selectional();
    }                               
    }                                           

    private void SearchFieldKeyReleased(java.awt.event.KeyEvent evt) {                                        
       try {
          
          String query = ("select * from Properties where Available =? ");
          PreparedStatement search = con.prepareStatement (query);
          search.setString(1, SearchField.getText());
          ResultSet rs = search.executeQuery();
          
          if (rs.next()) {
          
        String PropertyID = rs.getString(1);
        String LastName = rs.getString(2);
        String FirstName = rs.getString(3);
        String PhoneNumber = rs.getString(4);
        String RentPaid = rs.getString(5);
        String Email = rs.getString(6);
            
       
        
        IDTxt.setText(ID);
         LNTxt.setText(LastName);
        FNTxt.setText(FirstName);
        PNTxt.setText(PhoneNumber);
        RPTxt.setText(RentPaid);
        ETxt.setText(Email);
           //Tenants2.setModel(DbUtils.resultSetToTableModel(rs)); 
          }

 
                }
      catch (SQLException E) {
          E.printStackTrace();
      }
      selectional();
    }                               
    }                                       
